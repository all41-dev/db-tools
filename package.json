{
  "name": "@all41-dev/db-tools",
  "version": "1.3.4",
  "description": "manage databade updates",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"no test specified\" && exit 0",
    "lint": "eslint src/**",
    "start": "node ./dist/test.js",
    "build": "tsc",
    "ncu:view": "ncu -x ,sequelize,typescript",
    "ncu:upd": "ncu -x sequelize,typescript -u && npm i",
    "preversion": "run-p test lint build",
    "postversion": "git push && git push --tags && npm publish --access public"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "database",
    "update",
    "db",
    "sequelize",
    "sequelize-typescript"
  ],
  "author": "all41 <eric@all41.dev>",
  "license": "MIT",
  "dependencies": {
    "semver": "^7.3.8",
    "sequelize": "^6.29.0",
    "sequelize-typescript": "2.1.5",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/semver": "^7.3.13",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "mariadb": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  }
}
